package com.tobrun.android.notify.lib.notify;

public class Notify {

	private String mMessage = "Standard Message";
	private final int mDuration = 5000;
	private boolean isSticky = false;

	public Notify(final String message) {
		mMessage = message;
	}

	public String getMessage() {
		return mMessage;
	}

	public void setMessage(final String message) {
		mMessage = message;
	}

	public int getDuration() {
		return mDuration;
	}

	public int getLayoutId() {
		return mLayoutId;
	}

	public void setLayoutId(final int mLayoutId) {
		this.mLayoutId = mLayoutId;
	}

	public boolean isSticky() {
		return isSticky;
	}

	public void setIsSticky(final boolean sticky) {
		isSticky = sticky;
	}

}
